{
  "name": "the-traveler-times",
  "version": "2.0.0",
  "private": true,
  "description": "",
  "homepage": "https://github.com/empatheticbot/the-traveler-times#readme",
  "bugs": {
    "url": "https://github.com/empatheticbot/the-traveler-times/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/empatheticbot/the-traveler-times.git"
  },
  "license": "ISC",
  "author": "Steve Ledsworth <steve@empatheticbot.com>",
  "workspaces": [
    "./packages/types",
    "./packages/utils",
    "./packages/bungie-api-gateway",
    "./packages/d2-lost-sector-worker",
    "./packages/d2-meta-worker",
    "./packages/d2-nightfall-worker",
    "./packages/d2-oauth-worker",
    "./packages/d2-pgcr-worker",
    "./packages/d2-season-worker",
    "./packages/d2-trials-worker",
    "./packages/d2-vendor-worker",
    "./packages/d2-weekly-worker",
    "./packages/website-builder-worker",
    "./packages/website"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "build:web": "npm run build -w @the-traveler-times/bungie-api-gateway && npm run build -w @the-traveler-times/website",
    "build:workers": "npm run build -w @the-traveler-times/utils -w @the-traveler-times/bungie-api-gateway -w @the-traveler-times/d2-lost-sector-worker -w @the-traveler-times/d2-meta-worker -w @the-traveler-times/d2-nightfall-worker -w @the-traveler-times/d2-oauth-worker -w @the-traveler-times/d2-pgcr-worker -w @the-traveler-times/d2-season-worker -w @the-traveler-times/d2-trials-worker -w @the-traveler-times/d2-vendors-worker -w @the-traveler-times/d2-weekly-worker",
    "clean": "npm run clean --workspaces --if-present",
    "format": "npm run format --workspaces --if-present",
    "publish": "npm run build:workers && npm run publish --workspaces --if-present",
    "start": "npm run start --workspaces --if-present",
    "start:definition": "cd packages/d2-definition-worker && npm run start",
    "start:nightfall": "cd packages/d2-nightfall-worker && npm run start",
    "start:oauth": "cd packages/d2-oauth-worker && npm run start",
    "start:pgcr": "cd packages/d2-pgcr-crawler && npm run start",
    "start:season": "cd packages/d2-season-worker && npm run start",
    "start:trials": "cd packages/d2-trials-worker && npm run start",
    "start:vendors": "cd packages/d2-vendors-worker && npm run start",
    "start:web": "npm run start -w @the-traveler-times/website",
    "start:web:prod": "npm run start:prod -w @the-traveler-times/website",
    "start:workers": "npm run start -w @the-traveler-times/utils -w @the-traveler-times/bungie-api-gateway -w @the-traveler-times/d2-lost-sector-worker -w @the-traveler-times/d2-meta-worker -w @the-traveler-times/d2-nightfall-worker -w @the-traveler-times/d2-oauth-worker -w @the-traveler-times/d2-pgcr-worker -w @the-traveler-times/d2-season-worker -w @the-traveler-times/d2-trials-worker -w @the-traveler-times/d2-vendors-worker -w @the-traveler-times/d2-weekly-worker",
    "test": "npm run test --workspaces --if-present",
    "update": "npm-check -u",
    "version:major": "npm version major && npm version major --workspaces",
    "version:minor": "npm version minor && npm version minor --workspaces",
    "version:patch": "npm version patch && npm version patch --workspaces"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.3.1",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@tsconfig/node14": "1.0.1",
    "eslint": "^8.12.0",
    "npm-check": "5.9.2",
    "prettier": "^2.6.2",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "7.0.2",
    "terser": "^5.12.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "wrangler": "^0.0.24"
  }
}