<article class="meta">
  <h2 class="section-title">The Meta</h2>

  <h3 class="section-subtitle">Usage</h3>
  <ol class="inventory-item-list">
    {% for item in meta.topUsage %}
      {% set subtitle = [item.usage, meta.totalUsage] | getPrettyRatios %}
      <li class="inventory-item-container">{% include 'inventory-item.njk' %}</li>
    {% endfor %}
  </ol>

  <h3 class="section-subtitle">Kills</h3>
  <ol class="inventory-item-list">
    {% for item in meta.topKills %}
      {% set subtitle = [item.kills, meta.totalKills] | getPrettyRatios %}
      <li class="inventory-item-container">{% include 'inventory-item.njk' %}</li>
    {% endfor %}
  </ol>

  <h3 class="section-subtitle">Efficiency</h3>
  <ol class="inventory-item-list">
    {% for item in meta.topEfficiency %}
      {% set subtitle = [item.kills, item.usage] | getEfficiency %}
      <li class="inventory-item-container">{% include 'inventory-item.njk' %}</li>
    {% endfor %}
  </ol>
</article>
