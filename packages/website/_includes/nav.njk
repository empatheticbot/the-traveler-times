{% if showNav %}
  <nav class="nav">
    <section class="nav-group">
      {% for activity, date in nav %}
        {{ activity | safe}}
      {% endfor %}
    </section>
  </nav>
  <aside class="nav-aside">
    <p>
 The next reset is <until-date datetime="{{
      weekly.nextWeeklyReset
    }}"></until-date>. <a href="#season">{{
    season.currentSeason.displayProperties.name
  }}</a> ends <until-date datetime="{{
    season.currentSeason.endDate
  }}"></until-date>.</p>
    <p>
    Site last updated <since-date datetime="{{computed.buildDate}}"></since-date>
    </p>
    <p>
    If you like the site please <a href="https://buy.thetraveler.news">consider showing your support.</a> ❤️
  </p>
  </aside>
{% endif %}