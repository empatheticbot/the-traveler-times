<section class="section-container">
<header class="section-header">
  <h3 class="section-title">{{ sectionTitle }}</h3>
  <div class="section-divider"></div>
  {% if sectionDate %}
  <time class="section-date" datetime="{{ sectionDate | htmlDateString }}">
  {{ sectionTimeDescripter }} {{ sectionDate | relativeDate }}</time>
  {% endif %}
</header>
 {% block content %}
   {{ content | safe }}
 {% endblock %}
</section>
