{
  "private": true,
  "name": "@the-traveler-times/d2-pgcr-worker",
  "version": "1.14.0",
  "description": "Scheduled worker that crawls the Bungie Destiny2*Definition endpoints and stores the values in KV DESTINY_2_DEFINITIONS",
  "module": "./dist/index.mjs",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && rollup -c",
    "publish": "wrangler publish",
    "start": "wrangler dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write '**/*.{js,ts,njk,html,css,json,md}'",
    "clean:definitions": "wrangler kv:bulk delete --binding=DESTINY_2_DEFINITIONS definitions-bulk-delete.json"
  },
  "author": "Steve Ledsworth <steve@empatheticbot.com>",
  "dependencies": {
    "@the-traveler-times/bungie-api-gateway": "file:../bungie-api-gateway",
    "@the-traveler-times/utils": "file:../utils"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.1.1",
    "@cloudflare/wrangler": "^1.19.4",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@the-traveler-times/types": "file:../types",
    "@tsconfig/node14": "^1.0.1",
    "eslint": "^8.1.0",
    "prettier": "^2.4.1",
    "rollup": "^2.59.0",
    "rollup-plugin-terser": "7.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  }
}
